                <groupId>com.ocean</groupId>
                <artifactId>ocean-ddd-infrastructure</artifactId>
                <version>${project.version}</version>
            </dependency>
            <dependency>
                <groupId>com.ocean</groupId>
                <artifactId>ocean-ddd-service</artifactId>
                <version>${project.version}</version>
            </dependency>
