    <artifactId>ocean-ddd</artifactId>
    <packaging>pom</packaging>
    <version>1.0-SNAPSHOT</version>
    <modules>
        <module>ocean-ddd-start</module>
        <module>ocean-ddd-web</module>
        <module>ocean-ddd-acl</module>
        <module>ocean-ddd-client</module>
        <module>ocean-ddd-common</module>
